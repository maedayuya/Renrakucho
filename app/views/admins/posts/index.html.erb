<h1>投稿一覧</h1>
<table>
<thead>
	<tr>投稿者</tr>
	<tr>タイトル</tr>
	<tr>内容</tr>
</thead>
<tbody>
　<% @posts.each do |post| %>
  <tr>
	<td><%= post.admin.name %></td>
	<td><%= post.title %></td>
	<td><%= post.body %></td>
  <% end %>
  </tr>
</tbody>
</table>

<%= link_to "新規投稿画面へ",new_admins_post_path %>
<%= link_to "ログアウト" ,destroy_admin_session_path,method: :delete %>